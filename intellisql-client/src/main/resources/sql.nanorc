syntax "SQL" "\.sql$"
icolor blue "\\b(SELECT|INSERT|UPDATE|DELETE|FROM|WHERE|AND|OR|ORDER|BY|GROUP|LIMIT|OFFSET|JOIN|LEFT|RIGHT|INNER|OUTER|ON|AS|IN|IS|NULL|NOT|CREATE|DROP|TABLE|DATABASE|INDEX|VIEW|ALTER|ADD|COLUMN|SET|VALUES|INTO|UNION|ALL|DISTINCT|HAVING|CASE|WHEN|THEN|ELSE|END|EXISTS|LIKE|BETWEEN|SHOW|TABLES)\\b"
icolor magenta "\\b(INT|INTEGER|VARCHAR|CHAR|TEXT|DATE|DATETIME|TIMESTAMP|BOOLEAN|FLOAT|DOUBLE|DECIMAL|NUMERIC|BLOB)\\b"
color green "'.*'"
color green "("[^"]*")"
color red "--.*$"
color red "/\*.*\*/"
